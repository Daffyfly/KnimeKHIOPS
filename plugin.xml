<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/" factory-class="org.vincent.khiops.KhiopsLearningNodeFactory" id="org.vincent.khiops.KhiopsLearningNodeFactory"/>
   <node category-path="/" factory-class="org.vincent.khiops.KhiopsDictionnaryNodeFactory" id="org.vincent.khiops.KhiopsDictionnaryNodeFactory"/>
<node category-path="/" factory-class="org.vincent.khiops.KhiopsDictionnaryFromTableNodeFactory" id="org.vincent.khiops.KhiopsDictionnaryFromTableNodeFactory"/>
<node category-path="/" factory-class="org.vincent.khiops.KhiopsPredictorNodeFactory" id="org.vincent.khiops.KhiopsPredictorNodeFactory"/>
</extension>
   
</plugin>
